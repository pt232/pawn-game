{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "outputs": [".next/**", "storybook-static/**"],
      "dependsOn": ["^build"]
    },
    "lint": {},
    "test:e2e": {
      "cache": false
    },
    "test:storybook": {
      "cache": false
    },
    "test": {},
    "storybook": {
      "cache": false,
      "persistent": true
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
